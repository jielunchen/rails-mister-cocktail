<div class="container">
  <div class="row">
    <div class="col-md-6 mx-auto">
      <div class="card p-5">
        <h1>Add a new Cocktail</h1>
        <!-- <form action="/gardens/:garden_id/plants" method="post"></form> -->
        <%= simple_form_for([@dose, @ingredient]) do |f|  %>
          <%= f.input :name %>
          <%= f.input :image_url %>
          <%= f.button :submit, class: "btn btn-flat" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
